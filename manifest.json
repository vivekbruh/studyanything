{
  "name": "studyanything.",
  "description": "Instantly turn text into practice questions!",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/48.png",
    "72": "assets/72.png",
    "96": "assets/96.png",
    "144": "assets/144.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Generate practice problems!"
  },
  "permissions": ["contextMenus", "tabs", "storage"],
  "host_permissions": ["https://*/*"]
}